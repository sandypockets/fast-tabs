{
  "name": "React Chrome Extension",
  "description": "A starter for creating Chrome Extensions with React",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "background",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Fast Tabs"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}