{
  "name": "Fast Tabs",
  "description": "Open groups of your favourite websites in a flash.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "background",
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Fast Tabs"
  },
  "icons": {
    "16": "images/browsers16.png",
    "24": "images/browsers24.png",
    "32": "images/browsers32.png",
    "48": "images/browsers48.png",
    "64": "images/browsers64.png",
    "96": "images/browsers96.png",
    "128": "images/browsers128.png",
    "192": "images/browsers192.png",
    "256": "images/browsers256.png",
    "512": "images/browsers512.png"
  }
}